{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#C591E8",
          "style": "plain",
          "template": "({{ .Venv }}) ",
          "type": "python"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#C591E8>❯</><#69FF94>❯</>",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#D0666F",
          "properties": {
            "branch_icon": "",
            "fetch_status": false
          },
          "style": "plain",
          "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>)</>",
          "type": "git"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": " $ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}

